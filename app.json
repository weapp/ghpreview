    "GITHUB_ID": {
      "description": "Github application id from https://github.com/settings/applications/new",
    },
    "GITHUB_SECRET": {
      "description": "Github application secret from https://github.com/settings/applications/new",
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs#v63"
    }
  ]
}
